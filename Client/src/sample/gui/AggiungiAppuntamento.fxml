<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.ControllerAggiungiAppuntamento">
    <Label alignment="CENTER" contentDisplay="CENTER" layoutX="217.0" layoutY="14.0" prefHeight="65.0" prefWidth="286.0"
           text="Appuntamenti" textAlignment="CENTER">
        <font>
            <Font size="38.0"/>
        </font>
    </Label>
    <Label layoutX="81.0" layoutY="101.0" text="Nome">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <TextField fx:id="nomeAppAdd" layoutX="194.0" layoutY="98.0" prefHeight="33.0" prefWidth="375.0"/>
    <Label layoutX="81.0" layoutY="151.0" text="Descrizione">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <TextField fx:id="descrizioneAppAdd" layoutX="194.0" layoutY="148.0" prefHeight="33.0" prefWidth="375.0"/>
    <Label layoutX="81.0" layoutY="202.0" text="Data">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <TextField fx:id="dataAppAdd" layoutX="194.0" layoutY="199.0" prefHeight="33.0" prefWidth="375.0"/>
    <Button fx:id="aggiungiApp" layoutX="194.0" layoutY="425.0" mnemonicParsing="false" onAction="#aggiungi"
            prefHeight="33.0" prefWidth="176.0" text="Aggiungi"/>
    <Button fx:id="indietroAppAdd" layoutX="393.0" layoutY="425.0" mnemonicParsing="false" onAction="#indietro"
            prefHeight="33.0" prefWidth="176.0" text="Indietro"/>
    <Label layoutX="83.0" layoutY="253.0" text="Partecipanti">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <ListView fx:id="listviewAppAdd" layoutX="419.0" layoutY="253.0" prefHeight="151.0" prefWidth="150.0"/>
    <ComboBox fx:id="comboAppAdd" layoutX="194.0" layoutY="254.0" prefWidth="150.0"/>
    <Button fx:id="buttonAppAdd" layoutX="356.0" layoutY="254.0" mnemonicParsing="false" onAction="#addList"
            prefHeight="25.0" prefWidth="49.0" text="+"/>
</AnchorPane>
